{
    "name": "Trello : Big Edit",
        "version": "1.0",
        "manifest_version": 2,
        "description": "Makes the card edit window in Trello.com full screen",
        "icons": { "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png" },
        "content_scripts": [
        {
            "matches": ["http://trello.com/*","https://trello.com/*"],
            "js": ["trello_big_edit.user.js"]
        }
    ] 
}
